    issn       = "0006-3223",
    journal    = "Biological Psychiatry",
    language   = "en",
    month      = "feb",
    number     = 4,
    pages      = "307--315",
    series     = {Neuroimmune {{Mechanisms Related}} to {{Psychosis}}},
    shorttitle = {Prenatal {{Poly}}({{I}}},
    volume     = 75,
    year       = 2014,
